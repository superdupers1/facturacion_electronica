<p id="notice"><%= notice %></p>

<h1>Listing File Descriptions</h1>

<table>
  <thead>
    <tr>
      <th>Cantidad</th>
      <th>Unit</th>
      <th>Description</th>
      <th>Code</th>
      <th>Price</th>
      <th>Amount</th>
      <th>Document</th>
      <th>Product</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @file_descriptions.each do |file_description| %>
      <tr>
        <td><%= file_description.cantidad %></td>
        <td><%= file_description.unit %></td>
        <td><%= file_description.description %></td>
        <td><%= file_description.code %></td>
        <td><%= file_description.price %></td>
        <td><%= file_description.amount %></td>
        <td><%= file_description.document %></td>
        <td><%= file_description.product %></td>
        <td><%= link_to 'Show', file_description %></td>
        <td><%= link_to 'Edit', edit_file_description_path(file_description) %></td>
        <td><%= link_to 'Destroy', file_description, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New File description', new_file_description_path %>
